<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>InsertBookServlet</servlet-name>
        <servlet-class>InsertBookServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DisplayIndividualServlet</servlet-name>
        <servlet-class>DisplayIndividualServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DisplayAllServlet</servlet-name>
        <servlet-class>DisplayAllServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateBookServlet</servlet-name>
        <servlet-class>UpdateBookServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteBookServlet</servlet-name>
        <servlet-class>DeleteBookServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InsertBookServlet</servlet-name>
        <url-pattern>/InsertBookServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DisplayIndividualServlet</servlet-name>
        <url-pattern>/DisplayIndividualServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DisplayAllServlet</servlet-name>
        <url-pattern>/DisplayAllServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateBookServlet</servlet-name>
        <url-pattern>/UpdateBookServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteBookServlet</servlet-name>
        <url-pattern>/DeleteBookServlet</url-pattern>
    </servlet-mapping>
</web-app>